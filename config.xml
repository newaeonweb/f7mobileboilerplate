<?xml version='1.0' encoding='utf-8'?>
<widget id="com.newaeonweb.f7boilerplate" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>F7Boilerplate</name>
    <description>
        A sample Apache Cordova application build with Framework7 and F7Boilerplate.
    </description>
    <author email="fernando@newaeonweb.com.br" href="http://www.newaeonweb.com.br">
        Fernando MOnteiro
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
